// a document containing all the strange things Relaxed allows:
// inline comments
// line comments
// unquoted string
// hash keys with no values
// contiguous commas
{
	// line comment
	/* inline comment  */
	x:unquoted-string,
	
	hash-key-with-no-value,
	
	// extra commas
	,,,,,
	array:[xxx,yyy,,,zzz],
	hash: {}
}